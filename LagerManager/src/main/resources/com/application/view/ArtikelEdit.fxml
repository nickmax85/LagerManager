<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<AnchorPane prefHeight="705.0" prefWidth="787.0" xmlns="http://javafx.com/javafx/8.0.141" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.application.view.ArtikelEditController">
   <children>
    	<VBox spacing="10.0" AnchorPane.bottomAnchor="10.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="10.0" AnchorPane.topAnchor="0.0">
         <children>
            <fx:include fx:id="artikelData" source="ArtikelData.fxml" />
            	<HBox alignment="TOP_RIGHT" spacing="10.0">
               <children>
                  <Button focusTraversable="false" mnemonicParsing="false" onAction="#handleOk" prefWidth="125.0" text="OK" />
                <Button focusTraversable="false" mnemonicParsing="false" onAction="#handleCancel" prefWidth="125.0" text="Abbrechen" />
               </children>
            	</HBox>
         </children>
      </VBox>
   </children>
</AnchorPane>
